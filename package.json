{
  "name": "some-functional-functions",
  "version": "0.1.0",
  "description": "some haskell-wannabe functions in JS",
  "repository": "https://github.com/ging0044/func",
  "main": "func.js",
  "types": "func.d.ts",
  "scripts": {
    "test": "./node_modules/.bin/jest && npm run build",
    "build": "./node_modules/.bin/tsc --declaration --outdir . src/func.ts"
  },
  "keywords": [
    "functional",
    "haskell",
    "prelude",
    "typescript"
  ],
  "author": "Patrick Gingras",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^23.1.3",
    "jest": "^23.2.0",
    "ts-jest": "^22.4.6",
    "typescript": "^2.9.2"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
